  .pg-navbar {
  align-items: center;
  display: flex;
  justify-content: flex-end;
  padding: 0;
  position: relative;
  backgound: transparent !important;

  &__header {
    &-profile {
      display: none;
    }

    &-language {
      display: none;
    }

    &-settings {
      align-items: center;
      display: inherit;
      float: right;

      &__switcher {
        align-items: center;
        display: flex;
        height: calc(var(--gap) * 2.2);
        justify-content: flex-start;
        user-select: none;

        &__items {
          align-items: center;
          border: 1px solid #888888  !important;
          border-radius: 3px;
          display: flex;
          flex-direction: row;
          height: calc(var(--gap) * 2.84);
          justify-content: flex-start;
          width: calc(var(--gap) * 7.67);

          .switcher-item {
            align-items: center;
            cursor: pointer;
            display: flex;
            height: calc(var(--gap) * 2.84);
            justify-content: center;
            width: calc(var(--gap) * 3.84);

            &--active {
              background: transparent !important;
              height: 21px;
            }
          }
        }
      }

      &__account-dropdown {
        cursor: pointer;
        margin-right: calc(var(--gap) * 1);
        width: calc(var(--big-gap) * 1.2);

        &:hover {
          .dropdown-menu-language-selected {
            color: var(--cta-layer-color);
          }
        }

        .bootstrap-dropdown-menu {
          color: var(--dropdown-text-color);

          img {
            margin-right: 5px;
          }
        }

        a {
          font-size: calc(var(--gap) * 2.333);
          padding-bottom: calc(var(--gap) * 0.333);
          padding-top: calc(var(--gap) * 1.66);
        }

        .dropdown-menu {
          background-color: var(--dropdown-background-color);
          border-radius: var(--gap);
          cursor: pointer !important;
          margin: calc(var(--gap) * 0.333) 0 0 calc(var(--gap) * -0.167);
          padding-bottom: 0;
          padding-top: 0;
          position: absolute;
          z-index: 20000;

          .dropdown-item {
            align-items: center;
            display: flex;
            margin: 0;
            padding: calc(var(--gap) * 1.29) calc(var(--gap) * 1.66);
            transition: 0.2s;
            user-select: none;

            &:hover,
            &:focus,
            &:active {
              background-color: var(--dropdown-background-color-hover);
              color: var(--cta-layer-color);
              transition: 0.2s;
            }

            &:first-child {
              &:hover {
                border-top-left-radius: var(--gap);
                border-top-right-radius: var(--gap);
              }
            }

            &:last-child {
              &:hover {
                border-bottom-left-radius: var(--gap);
                border-bottom-right-radius: var(--gap);
              }
            }
          }
        }

        .dropdown-toggle::after {
          margin-left: calc(var(--gap) * 1.1);
          transition: 0.2s;
        }

        .show {
          .dropdown-toggle::after {
            transform: rotate(-180deg);
          }
        }
      }
    }
  }

  .dropdown-item {
    color: var(--dropdown-text-color);
  }

  .dropdown-item:hover {
    color: var(--dropdown-text-color-hover);
  }

  .dropdown {
    background-color: var(--dropdown-background-color) !important;
  }

  .dropdown-toggle {
    color: var(--dropdown-toggle-color);
    font-size: calc(var(--gap) * 3.33);
    line-height: normal;
    margin-left: var(--gap);
    text-align: center;
    user-select: none;

    .icon {
      color: var(--navbar-text-color);
      margin-left: calc(var(--gap) * 0.5);
    }

    &::after {
      display: none !important;
    }
  }
}

