.checkbox-input {
  align-items: center !important;
  display: flex !important;
  flex-direction: row;
  vertical-align: middle;
  
  
  
  font-size: 13px !important;
  
  .checkbox {
    --background: transparent !important;
    --border: 0.5px solid #a7a8af !important;
    --border-hover: #a7a8af !important;
    --border-active: #a7a8af !important;
    --tick: #fff !important;
    top: 15px;
    left: 5px;

    position: relative !important;

    input,
    svg {
      display: block !important;
      height: 21px !important;
      width: 21px !important;
    }

    input {
      -webkit-appearance: none !important;
      -moz-appearance: none !important;
      background: var(--background) !important;
      border: 2px solid #a7a8af !important;
      border-radius: 5px !important;
      box-shadow: inset 0 0 0 var(--s, 1px) var(--b, var(--border)) !important;
      cursor: pointer !important;
      margin: 0 !important;
      outline: none !important;
      padding: 0 !important;
      position: relative !important;
      transition: box-shadow 0.3s !important;

      &:hover {
        --s: 1px !important;
        --b: var(--border-hover) !important;
      }

      &:checked {
        --b: var(--border-active) !important;
      }
    }

    svg {
      fill: none;
      height: 21px;
      left: 0;
      pointer-events: none;
      position: absolute;
      stroke: var(--stroke, var(--border-active));
      stroke-linecap: round;
      stroke-linejoin: round;
      stroke-width: 2px;
      top: 0;
      transform: scale(var(--scale, 1)) translateZ(0);
      width: 21px;
    }

    &.path {
      input {
        &:checked {
          --s: 1px;

          transition-delay: 0.4s;

          & + svg {
            --a: 16.1 86.12;
            --o: 102.22;
          }
        }
      }

      svg {
        stroke-dasharray: var(--a, 86.12);
        stroke-dashoffset: var(--o, 86.12);
        transition: stroke-dasharray 0.6s, stroke-dashoffset 0.6s;
      }
    }

    &.bounce {
      font-size: 13px;
      --stroke: var(--tick);

      input {
        &:checked {
          --s: 11px;

          & + svg {
            animation: bounce 0.4s linear forwards 0.2s;
          }
        }
      }

      svg {
        --scale: 0;
      }
    }
  }

  @keyframes bounce {
    50% {
      transform: scale(1.2);
    }

    75% {
      transform: scale(0.9);
    }

    100% {
      transform: scale(1);
    }
  }
}

