    &__close {
      cursor: pointer;
      display: inline-block;
      height: calc(var(--gap) * 2.5);
      margin-left: calc(var(--gap) / 1.33);
      overflow: hidden;
      position: relative;
      width: calc(var(--gap) * 2.5);
  
      &:hover {
        &::before,
        &::after {
          background: var(--primary-text-color);
          transition: background 0.2s;
        }
      }
  
      &::before,
      &::after {
        background: var(--secondary-contrast-cta-color);
        content: "";
        height: 2px;
        left: 0;
        position: absolute;
        top: 55%;
        transition: background 0.2s;
        width: 100%;
      }
  
      &::before {
        transform: rotate(45deg);
      }
  
      &::after {
        transform: rotate(-45deg);
      }
    }
  
    &__pull-right {
      color: var(--secondary-contrast-cta-color);
      float: right;
      transition: 0.2s;
  
