    &__button-wrapper {
      min-height: calc(var(--gap) * 10);
    }

    .pg-profile-page__close {
      cursor: pointer;
      display: inline-block !important;
      height: calc(var(--gap) * 2.5);
      margin-top: calc(var(--gap) * 2.6);
      overflow: hidden;
      position: absolute;
      right: calc(var(--gap) * 3.33);
      width: calc(var(--gap) * 2.5);
    }
  }
}

@media (max-width: 767px) {
  .pg-profile-page-header-first {
    display: none;
  }

  tr {
    font-size: 0.8em;
    padding-left: 5px;
    padding-right: 5px;
  }

  .pg-profile-page__api-keys {
    .cr-table {
      tr,
      td,
      th {
        padding-left: 5px !important;
        padding-right: 5px !important;
      }
    }
  }

  .pg-profile-page__api-keys__state-checkbox {
    display: none;
  }
}

@media (max-width: 460px) {
  .pg-profile-page-header-first {
    display: none;
  }

  tr {
    font-size: 0.6em;
    padding-left: 5px;
    padding-right: 5px;
  }
}

@media (max-width: 360px) {
  .pg-profile-page__text-success::after {
    display: none;
  }

  tr,
  td {
    font-size: 0.5em;
  }
}


.pg-profile-page-verification {
	width: 100%;
	height: 20px;
	align-items: center;
		display: flex;

	&__title {
		color: [var(--primary-text-color)];
		font-size: 26px;
		height: 30px;
		line-height: 26px;
		margin: 0 !important;
	}

	&__progress-bar {
		align-items: center;
		display: flex;
		height: 50px;
		justify-content: flex-start;

		width: 100%;

		&__step {
			align-items: center;
			border-radius: 1px;
			cursor: default;
			display: flex;
			font-size: 16px;
			font-weight: bold; 
			height: 30px;
			justify-content: center;
	
			margin: 0 1px;
			padding-top: 1px;
			width: 100%;

			svg {
				margin-left: 5px;
				max-height: 9px;
				max-width: 9px;
			}

			&:first-child {
				border-bottom-left-radius: 3px;
				border-top-left-radius: 3px;
			}

			&:last-child {
				border-bottom-right-radius: 3px;
				border-top-right-radius: 3px;
			}

			&--verified {
				background: #009991 ;
				color: var(--primary-text-color);
        opacity: 0.81;
			}

			&--blocked {
				background: var(--icons);
				color: var(--contrast-cta-color-level-4);
        opacity: 0.81;

				span {
					color: var(--contrast-cta-color-level-4);
				}
			}

			&--active {
				background: #FDA736;
				color: var(--primary-text-color);
        opacity: 0.81;
				text-decoration: underline;

				span {
					color: var(--primary-text-color);
				}
			}

			&--pending {
				background: gray;
				color: var(--primary-text-color);

				span {
					color: var(--primary-text-color);
				}

				svg > path {
					fill: var(--primary-text-color);
					stroke: var(--primary-text-color);
				}
			}

			&--rejected {
				background: var(--system-red);
				color: var(--primary-text-color);
				text-decoration: underline;

				span {
					color: var(--primary-text-color);
					text-decoration: underline;
				}

				svg > path {
					fill: var(--primary-text-color);
					stroke: var(--primary-text-color);
				}
			}
		}
	}

	&__step {
		align-items: center;
		display: flex;
		height: 50px;
		justify-content: flex-start;
		flex-direction: column;

	
		
	

		&:last-child {
			margin-bottom: 0;
		}

		&--blocked {
			opacity: 0.36;
			pointer-events: none;
			user-select: none;
			flex-direction: column;
		}

		&--rejected {
			.pg-profile-page-verification__step__info__title {
				color: var(--primary-text-color);
			}
		}

		&__info {
			align-items: flex-start;
			display: flex;
			flex-direction: column;
			justify-content: flex-start;

			&__title {
				color: var(--primary-text-color);
				font-size: 15px;
				line-height: 15px;
				flex-direction: column;
			}

			&__subtitle {
				font-size: 12px;
				letter-spacing: 0.5px;
				line-height: 12px;
				margin-top: 8px;
				opacity: 0.72;
				flex-direction: column;
			}
		}

		&__label {
			align-items: center;
			display: flex;
			flex-direction: column;

			justify-content: flex-end;
			width: 100px;

			a,
			span {
				font-size: 12px;
				line-height: 15px;
				flex-direction: column;
			}

			svg {
				margin-left: 9px;
				max-height: 12px;
				max-width: 12px;
				flex-direction: column;
			}

			&--pending {
				color: var(--system-yellow);

				span {
					color: var(--system-yellow);
					flex-direction: column;
				}
			}

			&--verified {
				color: var(--system-green);

				span {
					color: var(--system-green);
					flex-direction: column;
				}
			}

			&--rejected {
				a {
					color: var(--system-red);
					text-decoration: underline;
				}

				span {
					color: var(--system-red);
					text-decoration: underline;
				}
			}
		}

		&__button {
			align-items: center;
			border-radius: 4px;
			display: flex;
			flex-direction: column;
			height: 30px;
			justify-content: center;
			width: 100px;

			a,
			span {
				align-items: center;
				color: var(--system-green);
				display: flex;
				flex-direction: column;
				font-size: 14px;
				font-weight: var(--button-font-weight);
				height: 100%;
				justify-content: center;
				letter-spacing: 1px;
				line-height: 16px;
				text-decoration: none;
				text-transform: uppercase;
				user-select: none;
				width: 100%;
			}

			&--active,
			&--blocked {
				background: purple !important;
				flex-direction: column !important;

				a {
					color: var(--primary-text-color);
					font-weight: bold;
					flex-direction: column;
				}

				span {
					color: var(--primary-text-color);
				}
			}

			&--active:hover {
				background: var(--button-primary-cta-background-color-hover);
			}
		}

		.tooltip-hover {
			background-color: var(--body-background-color-level-7);
			border-radius: 4px;
			color: var(--primary-text-color);
			display: none;
			font-size: 14px;
			font-weight: 600;
			height: max-content;
			letter-spacing: 0.02em;
			line-height: 20px;
			max-width: 256px;
			padding: 15px 24px;
			position: fixed;
			text-align: center;
			width: max-content;
			z-index: 10;

			&--visible {
				display: block;
				overflow: hidden;
				position: fixed;
			}
		}
	}
}




.cr-search {
	margin: var(--gap);
	padding: var(--gap);
	width: calc(100% - var(--gap) * 5);

	&__icon {
		padding: var(--gap);

		img {
			height: 17px;
			width: 17px;
		}
	}

	&__input {
		border: none;
		border-bottom: 1px solid #F5F5F5;
		color: #F5F5F5;
		padding: var(--gap);

		&::placeholder {
			color: #F5F5F5;
		}

		&:focus {
			outline: none;
		}
	}
}

#wallet-list-screen {
