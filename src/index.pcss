.pg-combined-order-book {
	height: 100%;
	overflow: hidden;

	&-loader {
		align-items: center;
		display: flex;
		height: 100%;
		justify-content: center;
		padding-bottom: calc(var(--gap) * 4.5);
	}

	&--no-data-first {
		.cr-order-book {
			&:first-child {
				.cr-table {
					&__body {
						tr {
							td:nth-child(2) {
								bottom: 0;
								color: var(--secondary-contrast-cta-color);
								left: 0;
								padding-left: 0 !important;
								padding-right: 0 !important;
								position: absolute;
								right: 0;
								text-align: center !important;
								width: 100% !important;
							}
						}
					}
				}
			}
		}
	}

	&--no-data-second {
		.cr-order-book {
			&:last-child {
				.cr-table {
					&__body {
						tr {
							td:nth-child(2) {
								color: var(--secondary-contrast-cta-color);
								left: 0;
								padding-left: 0 !important;
								padding-right: 0 !important;
								position: absolute;
								right: 0;
								text-align: center !important;
								width: 100% !important;
							}
						}
					}
				}
			}
		}
	}

	.cr-table {
		&__head {
			th {
				width: 33% !important;
				position: absolute !important;
			}

			th:last-child {
				text-align: center !important;
			}
		}

		&__body {
			td {
				color: var(--table-text-color);
				width: 33% !important;

				span:first-child.cr-decimal__opacity {
					opacity: 0.6;
				}
			}
		}
	}

	.cr-combined-order-book {
		.cr-table {
			&__head {
				th:first-child {
					text-align: right;
				}

				th:nth-child(2) {
					text-align: right;
				}
			}

			&__body {
				td:first-child {
					text-align: right;
				}

				td:nth-child(2) {
					text-align: right;
				}

				@media screen and (max-width: 768px) {
					td {
						padding-top: calc(var(--gap) * 0.7) !important;
					}
				}
			}

			&-background {
				&__row {
					min-width: 1px;
					padding: 22px 0;
				}
			}
		}

		&__small {
			height: 100%;
			position: relative;

			.cr-combined-order-book__market {
				left: 0;
				margin-top: 0;
				position: absolute;
				right: 0;
				top: calc(50% - 10px) !important;
			}

			.cr-order-book {
				border-bottom: 1px solid var(--order-book-last-market-price-border-color);
				display: flex;
				height: calc(50% - 25px) !important;
				position: absolute;

				.cr-table {
					thead {
						display: table;
						position: fixed;
						top: 50px;
						width: 100%;
						z-index: 2222;
					}

					&__body {
						cursor: pointer;

						@media screen and (max-width: 768px) {
							td {
								padding-top: calc(var(--gap) * 2.5) !important;
							}
						}
					}
				}

				&:first-child {
					flex-direction: column-reverse;
					left: 0;
					overflow-x: hidden;
					right: 0;
					top: 0;

					.cr-table {
						td {
							width: 33% !important;
						}

						&-background {
							left: 0;
							right: 0;
							top: calc(var(--gap) * 7);
						}

						tbody::before {
							content: '';
							display: block;
							height: calc(var(--gap) * 7);
						}

						&-container {
							margin-top: auto;
							width: 100%;
						}
					}
				}

				&:last-child {
					border-top: 1px solid var(--order-book-last-market-price-border-color);
					left: 0;
					right: 0;
					top: calc(50% + 35px);

					.cr-table {
						&-container {
							width: 100%;
						}
					}

					tbody::after {
						content: '';
						display: block;
						height: calc(var(--gap) * 7);
					}
				}
			}

			.cr-table-background {
				&__row {
					padding-bottom: calc(var(--gap) * 1.5);
				}
			}
		}

		&__large {
			height: calc(100% - 85px);
			overflow-y: scroll;

			.cr-order-book {
				padding-right: 0;
			}

			.cr-table {
				&__head {
					display: table;
					position: fixed;
					width: 50%;
					z-index: 222;

					th:first-child {
						text-align: left;
					}
				}

				tbody {
					td:first-child {
						text-align: left;
					}

					&::before {
						content: '';
						display: block;
						height: calc(var(--gap) * 6);
					}
				}
			}

			&-market {
				border-top: 1px solid var(--order-book-last-market-price-border-color);
				position: fixed;
			}

			.cr-table-background {
				top: 36px;

				&__row {
					margin-right: 1px;
					padding-bottom: calc(var(--gap) * 1.5);
				}
			}
		}
	}

	.head-text {
		font-size: 20px;
		font-weight: 500;
	}

	.change-icon {
		margin-right: 0.5em;
	}
}

@-webkit-keyframes flash_red {
	from { background-color: #ef5350  ; }
	to { background-color: transparent; }
  }
  @-moz-keyframes flash_red {
	from { background-color: #ef5350  ; }
	to { background-color: transparent; }
  }
  @-o-keyframes flash_red {
	from { background-color: #ef5350  ; }
	
	to { background-color: transparent; }
  }
  @keyframes flash_red {
	from { background-color: #ef5350 ; }
	to { background-color: transparent; }
  }


@-webkit-keyframes flash_green {
	from { background-color: #13b887 ; }
	to { background-color: transparent; }
  }
  @-moz-keyframes flash_green {
	from { background-color: #13b887 ; }
	to { background-color: transparent; }
  }
  @-o-keyframes flash_green {
	from { background-color: #13b887 ; }
	to { background-color: transparent; }
  }
  @keyframes flash_green {
	from { background-color: #13b887; }
	to { background-color: transparent; }
  }

  @-webkit-keyframes flashX {
	from { background-color: #FDA736; }
	to { background-color: transparent; }
  }
  @-moz-keyframes flashX {
	from { background-color: #FDA736; }
	to { background-color: transparent; }
  }
  @-o-keyframes flashX {
	from { background-color: #FDA736; }
	to { background-color: transparent; }
  }
  @keyframes flashX {
	from { background-color: #FDA736; }
	to { background-color: transparent; }
  }

  .flash_green {
	-webkit-animation: flash_green 2s ; /* Safari 4+ */
	-moz-animation:    flash_green 2s ; /* Fx 5+ */
	-o-animation:      flash_green 2s ; /* Opera 12+ */
	animation:         flash_green 2s ; /* IE 10+ */
  }

  .flash_red {
	-webkit-animation: flash_red 2s ; /* Safari 4+ */
	-moz-animation:    flash_red 2s ; /* Fx 5+ */
	-o-animation:      flash_red 2s ; /* Opera 12+ */
	animation:         flash_red 2s ; /* IE 10+ */
  }

  .flashX {
	-webkit-animation: flashX 2s ; /* Safari 4+ */
	-moz-animation:    flashX 2s ; /* Fx 5+ */
	-o-animation:      flashX 2s ; /* Opera 12+ */
	animation:         flashX 2s ; /* IE 10+ */
  }




.cr-tab-panel {
	display: flex;
	flex-direction: row;


	.cr-tab {
		color: #c0c0c0;
		font-size: 14px;
		padding: 10px;
		position: relative;
		text-align: center;
		
		transition: 0.1s;
		

		&__fixed {
			& .cr-tab {
				flex: 5;
			}
		}

		&:focus {
			background-color: transparent;
			outline: none;
		}

		&__active {
			color: var(--base);
		}

		&__hidden {
			display: none;
		}

		&:not(&__disabled) {
			cursor: pointer;
		}

		&__disabled {
			opacity: 0.2;
		}

		&__pointer {
			display: none !important;
			background: none !important;
			bottom: -2px;
			height: 0.25em;
			left: 0;
			position: absolute;
			width: 100%;
			z-index: 1;
		}
	}

	&__navigation-container {
		display: flex;
		margin-bottom: 10px;
		width: 100% !important;
		flex-direction: space-between !important;
		justify-content: center;

		&-navigation {
			box-shadow: inset 0 -1px var(--base);
			display: flex;
			flex-direction: space-between !important;
			padding-bottom: 1px;
			width: 100% !important;
			
			position: relative;

			.cr-tab-panel--fixed & {
				flex: 1;
			}
		}
	}

	.cr-tab-content {
		display: flex;
		flex-direction: column !important;
		position: relative !important;

		&:not(.cr-tab-content__active) {
			display: none;
		}

		&__active {
			position: absolute !important;
			padding: 0 !important;
			display: flex !important;
			flex-direction: flex-start !important;
			width: 100% !important;
			margin-top: 10px !important;
			background: transparent !important;

		}
	}
}

