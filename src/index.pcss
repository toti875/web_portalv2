			border-top: 1px solid #a7a8af;
		}

		th {
			background-color: transparent !important;
			color: #F5F5F5;
		}

		th&:not(:first-child) {
			text-align: center;
		}

		tr td&:not(:first-child) {
			text-align: center;
		}

		tr&:hover td {
			background-color: #41455c;
			color: #F5F5F5;
		}

		th&:last-child {
			text-align: center;
		}
	}

	.pagination {
		display: flex;
		font-size: 13;
		justify-content: space-between;
		margin-top: 10px;

		.pagination-page_number,
		.pagination-page_select {
			color: #F5F5F5;

			select,
			input {
				background-color: transparent !important;
				border: 1px solid #009991 !important;
				border-radius: 2px;
				color: #F5F5F5;
				font-size: 13;
		
				outline: none;
				padding: 0.3rem 0.5rem 0.3rem 0.5rem;
				padding: 0.5rem;
				-webkit-transition: width 0.4s ease-in-out;
				transition: all 0.2s ease-in-out;
				width: 200px;

				/* When the input field gets focus, change its width to 100% */
				&:focus {
					border: 1px solid #009991 !important;
					width: 200px;
				}
			}
		}

		.pagination-button-box {
			button {
				background-color: #2d2e3d;
				border: none;
				border-radius: 5px;
				color: #F5F5F5;
				cursor: pointer;
				height: 25px;
				margin-right: 5px;
				outline: none;
				transition: ease-in-out 0.3s;
				width: 25px;

				&:disabled {
					background-color: #313445;
					color: #49619e;
					cursor: not-allowed;
				}
			}
		}
	}

	.empty {
		background-color: transparent;
		height: 200px;
		left: 0;
		padding: 50px 0;
		position: absolute;
		top: 35px;
		width: 100%;
		display: none;
	}
}


