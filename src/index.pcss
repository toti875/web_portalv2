				}
			}

			&__col-right {
				h4 {
					background: #313445;
					color: #fff;
					font-size: 18px;
					line-height: 27px;
					margin-bottom: 0;
					padding: 12px;
				}

				&__commision {
					&-email {
						-webkit-box-orient: vertical;
						display: -webkit-box;
						-webkit-line-clamp: 2;
						overflow: hidden;
					}
				}
			}
		}

		&-pagination {
			display: flex;
			flex-direction: row;
			justify-content: center;
			margin-top: 10px;
			padding-left: 0;

			li {
				align-items: center;
				background-color: transparent;
				border: none;
				border-radius: 50%;
				color: #fff;
				cursor: pointer;
				display: flex;
				font-size: 14px;
				height: 30px;
				justify-content: center;
				outline: none;
				text-align: center;
				transition: all 0.3s;
				width: 30px;
			}

			.active {
				background-color: rgb(47, 182, 126) !important;
			}
		}
	}
}

.pg-mobile-user-info {
	align-items: flex-end;
	background: var(--mobile-body-background-color);
	display: flex;
	flex-direction: row;
	height: 87px;
	justify-content: space-between;
	margin: 4px 0;
	padding: 22px 16px;
	width: 100%;

	&__details {
		align-items: flex-start;
		display: flex;
		flex-direction: column;
		height: 100%;
		justify-content: flex-start;
		width: 100%;

		span {
			color: var(--primary-text-color);
			font-size: 16px;
			letter-spacing: 0.1px;
			line-height: 16px;
			margin-bottom: 12px;

			&:last-child {
				font-weight: 500;
				margin-bottom: 0;
			}
		}
	}

	&__logout-button {
		align-items: center;
		border: 1px solid #009991;
		border-radius: 4px;
		color: #009991;
		display: flex;
		font-size: 11px;
		font-weight: normal;
		height: 36px;
		justify-content: center;
		letter-spacing: 0.1px;
		min-width: 98px;
		padding: 0 6px;
		width: max-content;
	}
}
